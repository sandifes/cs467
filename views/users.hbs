<a href="/">home</a>
<table>
    <thead>
        <tr>
            <th>Username</th>
            <th>Email Address</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Department</th>
            <th>Account Type</th>
        </tr>
    </thead>
    <tbody>
        {{#each users}}
            <tr>
                <th>{{username}}</th>
                <th>{{email}}</th>
                <th>{{firstname}}</th>
                <th>{{lastname}}</th>
                <th>{{dept_id}}</th>
                <th>{{account_type}}</th>
            </tr>
        {{/each}}
    </tbody>
</table>